{
  "name": "Brackets-Electron",
  "productName": "Brackets-Electron",
  "version": "1.8.0-0",
  "apiVersion": "1.8.0",
  "homepage": "http://brackets.io",
  "issues": {
    "url": "http://github.com/adobe/brackets/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/brackets.git",
    "branch": "",
    "SHA": ""
  },
  "main": "./dist/index.js",
  "bin": {
    "brackets": "./dist/index.js"
  },
  "scripts": {
    "build": "rimraf ./dist && gulp build && tsc",
    "install": "./pre-rebuild.sh && electron-rebuild && npm run build",
    "start": "electron .",
    "bundle-mac": "./bundle.sh darwin x64 1.3.3",
    "bundle-win": "./bundle.sh win32 x64 1.3.3",
    "bundle-linux": "./bundle.sh linux x64 1.3.3",
    "dev": "rimraf ./dist && concurrently --kill-others \"gulp watch\" \"tsc --watch\"",
    "test": "npm run build && tslint -c tslint.json --project tsconfig.json"
  },
  "dependencies": {
    "bluebird": "2.10.1",
    "fs-extra": "0.24.0",
    "isbinaryfile": "3.0.0",
    "lodash": "4.15.0",
    "portscanner": "1.0.0",
    "strip-bom": "2.0.0",
    "trash": "2.0.0",
    "typescript": "2.0.0",
    "watch": "0.16.0",
    "ws": "0.8.0"
  },
  "optionalDependencies": {
    "fsevents": "1.0.6",
    "pathwatcher": "6.3.1"
  },
  "devDependencies": {
    "@types/electron": "1.3.19",
    "@types/fs-extra": "0.0.30",
    "@types/lodash": "4.14.32",
    "@types/node": "6.0.36",
    "@types/ws": "0.0.29",
    "concurrently": "2.2.0",
    "electron-builder": "5.26.0",
    "electron-packager": "7.6.0",
    "electron-prebuilt": "1.3.3",
    "electron-rebuild": "1.2.0",
    "grunt": "0.4.5",
    "grunt-cleanempty": "1.0.3",
    "grunt-cli": "0.1.13",
    "grunt-contrib-clean": "0.6.0",
    "grunt-contrib-concat": "0.5.1",
    "grunt-contrib-copy": "0.8.1",
    "grunt-contrib-cssmin": "0.14.0",
    "grunt-contrib-htmlmin": "0.4.0",
    "grunt-contrib-less": "1.0.1",
    "grunt-contrib-requirejs": "0.4.4",
    "grunt-contrib-uglify": "0.9.2",
    "grunt-contrib-watch": "0.6.1",
    "grunt-eslint": "18.1.0",
    "grunt-targethtml": "0.2.6",
    "grunt-usemin": "3.1.1",
    "gulp": "3.9.1",
    "gulp-watch": "4.3.9",
    "load-grunt-tasks": "3.5.0",
    "q": "1.4.1",
    "rimraf": "2.5.4",
    "semver": "5.0.3",
    "tslint": "3.14.0",
    "xmldoc": "0.3.2"
  },
  "licenses": [
    {
      "type": "MIT",
